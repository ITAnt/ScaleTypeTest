<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.itant.scaletypetest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FitCenterActivity"/>
        <activity android:name=".CenterActivity"/>
        <activity android:name=".CenterCropActivity"/>
        <activity android:name=".CenterInsideActivity"/>
        <activity android:name=".FitEndActivity"/>
        <activity android:name=".FitStartActivity"/>
        <activity android:name=".FitXYActivity"/>
        <activity android:name=".MatrixActivity"/>
    </application>

</manifest>