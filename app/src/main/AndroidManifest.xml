<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.itant.scaletypetest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FitCenterActivity"
            android:label="fitCenter"/>
        <activity android:name=".CenterActivity"
            android:label="center"/>
        <activity android:name=".CenterCropActivity"
            android:label="centerCrop"/>
        <activity android:name=".CenterInsideActivity"
            android:label="centerInside"/>
        <activity android:name=".FitEndActivity"
            android:label="fitEnd"/>
        <activity android:name=".FitStartActivity"
            android:label="fitStart"/>
        <activity android:name=".FitXYActivity"
            android:label="fitXy"/>
        <activity android:name=".MatrixActivity"
            android:label="matrix"/>
    </application>

</manifest>